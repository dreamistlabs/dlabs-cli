{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "project": ["./jsconfig.json"],
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "import/no-anonymous-default-export": "off",
    "no-unused-vars": "warn",
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  },
  "settings": {}
}
